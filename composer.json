{
    "name": "hexlet/code",
    "version": "1.0.0",
    "description": "gendiff package",
    "type": "project",
    "require": {
        "php": ">=8.2",
        "docopt/docopt": "^1.0",
        "lstrojny/functional-php": "^1.17",
        "symfony/yaml": "*"
    },
    "autoload": {
        "psr-4": {
            "Differ\\": "src/"
        },
        "files": [
            "src/Parser.php",
            "src/Differ.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Differ\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/gendiff"
    ],
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.13",
        "phpunit/phpunit": "^12.2",
        "phpstan/phpstan": "^1.10"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "mendax1337",
            "email": "amendax@gmail.com"
        }
    ]
}
